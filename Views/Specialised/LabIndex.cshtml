            <th>
                @Html.DisplayNameFor(model => model.physician)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.testName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.results)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.interpretation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.recommendations)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
